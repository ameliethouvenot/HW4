<h1><%= @place["name"] %></h1>

<h2>My Posts</h2>
<% for entry in @entries %>
  <h3><%= entry["title"] %></h3>
  <h4><%= entry["occurred_on"] %></h4>
  <p><%= entry["description"] %></p>
<% end %>

<p>
  <button class="btn btn-dark" href="/entries/new?place_id=<%= @place["id"] %>">New Entry</button>
</p>

<p>
  <button class="btn btn-dark" href="/places">Back to All Places</button>
</p>